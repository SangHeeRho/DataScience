#-------------Method 01-----------------
def PriceList():
    num = int(input("구매번호를 입력하세요.(1~6): "))
    if num == '6':
	    print('종료합니다.')
	    exit()
    elif 0<num<6:
	    print("%s는 %d원입니다."%(menu[num-1],money[num-1]))
	    # print(f'{menu[i]}는 {money[i]}원 입니다!')
    else:
	    print('구매번호를 확인하세요!')

print(PriceList())


