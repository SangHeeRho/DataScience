def DelID():
    inputID = input('ID 입력해주세요. :')
    if inputID in id_list:
        deleteIDList.append(inputID)
    else:
        print('해당 아이디가 없습니다.')

def PrintList():
    for i in range(len(id_list)):
	    dic[id_list[i]]=score_list[i]
    for i in range(len(dic)):
		print(f'{id_list}')
